<?xml version='1.0' encoding='utf-8'?>
<widget id="com.biocaching.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Biocaching</name>
    <description>
        A Biocaching prototype that shows various screens.
    </description>
    <author email="peter@biocaching.com" href="http://biocaching.com">
        Biocaching
    </author>
    <content src="index.html" />
    <icon src="www/style/icon.png"/>
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
