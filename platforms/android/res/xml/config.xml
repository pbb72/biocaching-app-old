<?xml version='1.0' encoding='utf-8'?>
<widget id="com.biocaching.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Biocaching</name>
    <description>
        A Biocaching prototype that shows various screens.
    </description>
    <author email="peter@biocaching.com" href="http://biocaching.com">
        Biocaching
    </author>
    <content src="index.html" />
    <icon src="style/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
